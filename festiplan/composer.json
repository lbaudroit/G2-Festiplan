{
    "name": "g2/festiplan",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/the-yasmf-project/yasmf"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-pdo": "*",
        "yasmf/yasmf": ">=1.6.1"
    },
    "config": {
        "vendor-dir": "./libs/"
    },
    "autoload": {
        "psr-4": {
            "controllers\\": "controllers",
            "services\\": "services",
            "application\\": "application",
            "views\\": "views"
        }
    }
}
